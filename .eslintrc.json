{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:react-hooks/recommended",
        "prettier"
    ],
    "plugins": ["react", "react-hooks"],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "es2020": true
    },
    "rules": {
        "react/react-in-jsx-scope": "off",
        "trailing-comma": "off",
        "no-unused-vars": "warn"
    }
}
